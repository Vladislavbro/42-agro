# === Ключи API ===

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# DeepSeek API Key
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_BASE=https://api.deepseek.com

# Gemini API Key
GEMINI_API_KEY=your_gemini_api_key

# === Модели LLM (можно оставить дефолтные) ===
DEEPSEEK_MODEL_NAME=deepseek-chat
GEMINI_MODEL_NAME=gemini-2.0-flash
OPENAI_MODEL_NAME=gpt-4.1-mini

# === Какой LLM использовать ===
# Возможные значения: deepseek, gemini, openai
PRIMARY_LLM_PROVIDER=openai

# === Google Drive загрузка Excel-файлов ===
# Ссылка на папку, выданную организаторами
GOOGLE_DRIVE_FOLDER_URL=https://drive.google.com/drive/folders/your_folder_id_here

# === Google Sheets (не используется, можно оставить закомментированным) ===
# GOOGLE_SHEET_ID=your_google_sheet_id
# GOOGLE_APPLICATION_CREDENTIALS=path/to/your/credentials.json
# GOOGLE_SHEET_RANGE=Sheet1!A1
